@model IEnumerable<ContosoMvcApp.ViewModels.EnrollmentDateGroup>

@{
    ViewBag.Title = "About";
}

<h2>Student Body Statistics</h2>

<table>
    <tr>
        <th>
            Enrollment Date
        </th>
        <th>
            Students
        </th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(ItemModel => item.EnrollmentDate)
        </td>
        <td>
            @Html.DisplayFor(ItemModel => item.StudentCount)
        </td>
    </tr>
}
</table>